<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AntiActivity"
            android:label="Anti Album"
            android:parentActivityName=".LoudActivity" />
        <activity android:name=".UnapologeticActivity"
            android:label="Unapologetic Album"
            android:parentActivityName=".LoudActivity"/>
        <activity android:name=".LoudActivity"
            android:label="Loud Album"
            android:parentActivityName=".LoudActivity"/>
        <activity android:name=".TalkthattalkActivity"
            android:label="Talk that talk Album"
            android:parentActivityName=".LoudActivity"/>
    </application>

</manifest>